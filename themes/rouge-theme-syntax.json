{
	"name": "Rouge",
	"type": "dark",
	"include": "./rouge-theme-editor.json",
	"tokenColors": [
		{
			"settings": {
				"foreground": "#A7ACB9",
				"background": "#172030"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"unused.comment",
				"wildcard.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#64727d"
			}
		},
		{
			"name": "Function declarations",
			"scope": [
				"entity.name.function",
				"support.constant.handlebars"
			],
			"settings": {
				"foreground": "#6e94b9f2",
				"fontStyle": "bold"
			}
		},
		{
			// "name": "Support functions",
			"scope": [
				"support.function",
				"keyword.operator"
			],
			"settings": {
				// "foreground": "#7ea9a9",
				"foreground": "#94b8b8",
				"fontStyle": "regular"
			}
		},
		{
			"name": "Types declaration and references",
			"scope": [
				"meta.return-type",
				"support.class",
				// "support.type",
				"entity.name.type",
				"entity.name.class",
				"storage.type.cs",
				"storage.type.generic.cs",
				"storage.type.modifier.cs",
				"storage.type.variable.cs",
				"storage.type.annotation.java",
				"storage.type.generic.java",
				"storage.type.java",
				"storage.type.object.array.java",
				"storage.type.primitive.array.java",
				"storage.type.primitive.java",
				"storage.type.token.java",
				"storage.type.groovy",
				"storage.type.annotation.groovy",
				"storage.type.parameters.groovy",
				"storage.type.generic.groovy",
				"storage.type.object.array.groovy",
				"storage.type.primitive.array.groovy",
				"storage.type.primitive.groovy"
			],
			"settings": {
				"foreground": "#f7987e",
				"fontStyle": "bold"
			}
		},
		{
			// "name": "Types declaration and references, TS grammar specific",
			"scope": [
				"meta.return.type",
				"meta.type.cast.expr",
				"meta.type.new.expr",
				"support.constant.math",
				"support.constant.dom",
				"support.constant.json",
				"entity.other.inherited-class",
				"string"
			],
			"settings": {
				"foreground": "#97a38f"
			}
		},
		{
			"name": "Control flow keywords",
			"scope": "keyword.control",
			"settings": {
				"foreground": "#b18bb1",
				"fontStyle": "regular"
			}
		},
		{
			"name": "Operator keywords",
			"scope": [
				"keyword",
				"keyword.operator.expression.delete",
				"keyword.operator.new",
				"keyword.operator.expression",
				"keyword.operator.cast",
				"keyword.operator.sizeof"
			],
			"settings": {
				"foreground": "#b18bb1",
				"fontStyle": "regular"
			}
		},
		{
			"name": "Operator keywords for C# and Java",
			"scope": [
				"keyword.cs",
				"keyword.java"
			],
			"settings": {
				"foreground": "#b18bb1",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage",
				"storage.modifier",
				"storage.type.js",
				"storage.type.function.js"
			],
			"settings": {
				"foreground": "#b18bb1"
				// "fontStyle": "italic"
			}
		},
		{
			"scope": [
				"variable.other.property",
				"variable.other.object",
				"meta.definition.variable.name",
				"support.variable"
			],
			"settings": {
				"foreground": "#ea7599"
			}
		},
		{
			"name": "Variable and parameter name",
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#bbb"
				// "foreground": "#ea7599"
			}
		},
		{
			"scope": [
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#bbb"
			}
		},
		{
			"name": "Object keys, TS grammar specific",
			"scope": [
				"meta.object-literal.key",
				"meta.object-literal.key entity.name.function"
			],
			"settings": {
				"foreground": "#ea7599"
			}
		},
		{
			"name": "Constants (null, boolean)",
			"scope": [
				"constant.language",
				"constant.numeric",
				"keyword.other.unit"
			],
			"settings": {
				"foreground": "#eabe9a"
			}
		},
		{
			"name": "Separators (key/value, namespace, inheritance, pointer, hash, slice, etc)",
			"scope": [
				"constant.other.symbol.hashkey punctuation.definition.constant.ruby",
				"entity.other.attribute-name.placeholder punctuation",
				"entity.other.attribute-name.pseudo-class punctuation",
				"entity.other.attribute-name.pseudo-element punctuation",
				"meta.group.double.toml",
				"meta.group.toml",
				"meta.object-binding-pattern-variable punctuation.destructuring",
				"punctuation.colon.graphql",
				"punctuation.definition.block.scalar.folded.yaml",
				"punctuation.definition.block.scalar.literal.yaml",
				"punctuation.definition.block.sequence.item.yaml",
				"punctuation.definition.entity.other.inherited-class",
				"punctuation.function.swift",
				"punctuation.separator.dictionary.key-value",
				"punctuation.separator.hash",
				"punctuation.separator.inheritance",
				"punctuation.separator.key-value",
				"punctuation.separator.key-value.mapping.yaml",
				"punctuation.separator.namespace",
				"punctuation.separator.pointer-access",
				"punctuation.separator.slice",
				"string.unquoted.heredoc punctuation.definition.string",
				"support.other.chomping-indicator.yaml"
			],
			"settings": {
				"foreground": "#bbb"
			}
		},
		{
			"name": "this, super, self, etc.",
			"scope": [
				"this.self",
				"keyword.expressions-and-types.swift",
				"keyword.other.this",
				"variable.language",
				"variable.other.readwrite.instance.ruby",
				"variable.parameter.function.language.special"
			],
			"settings": {
				"foreground": "#ea7599",
				"fontStyle": "italic"
			}
		},
		{
			"name": "HTML tags",
			"scope": [
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#ea7599",
				"fontStyle": "bold"
			}
		},
		{
			"name": "SCSS/LESS variables",
			"scope": [
				"variable.scss",
				"variable.less"
			],
			"settings": {
				"foreground": "#ea7599",
				"fontStyle": "regular"
			}
		},
		{
			"name": "CSS attribute parent selectors ('&')",
			"scope": [
					"entity.other.attribute-name.parent-selector"
			],
			"settings": {
					"foreground": "#fff",
					"fontStyle": "regular"
			}
		},
		{
			"name": "HTML/CSS attribute names",
			"scope": [
				"entity.other.attribute-name",
				"entity.other.attribute-name.class.css",
				"entity.other.attribute-name.id.css",
				"entity.other.attribute-name.pseudo-element.css"
			],
			"settings": {
				"foreground": "#eabe9a",
				"fontStyle": "regular"
			}
		},
		{
			"name": "Generic selectors (CSS/SCSS/Less/Stylus)",
			"scope": [
				"entity.name.tag.css",
				"meta.selector",
				"meta.selector.css"
			],
			"settings": {
				// "foreground": "#dcdcaa",
				"foreground": "#eabe9a",
				"fontStyle": "regular"
			}
		},
		{
			"name": "SCSS/LESS Keywords and Logical Operators",
			"scope": [
				"support.constant.media",
				"keyword.operator.logical.scss"
			],
			"settings": {
				"foreground": "#f7987e",
				"fontStyle": "regular"
			}
		},
		{
			"name": "CSS property value",
			"scope": [
				"support.constant.property-value",
				"support.constant.font-name",
				"support.constant.media-type",
				"constant.other.color.rgb-value",
				"constant.other.rgb-value",
				"support.constant.color",
				"meta.property-list.css",
				"meta.property-name.css",
				"meta.property-value.css"
			],
			"settings": {
				// "foreground": "#97a38f"
				"foreground": "#9BBA9A"
			}
		},
		{
			"scope": [
				"markup.error"
			],
			"settings": {
				"foreground": "#CC565EDF"
			}
		},
		{
			"scope": [
				"markup.inserted"
			],
			"settings": {
				"foreground": "#97a38f"
			}
		},
		{
			"scope": [
				"markup.deleted"
			],
			"settings": {
				"foreground": "#CC565EDF"
			}
		},
		{
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#FFB86C"
			}
		},
		{
			"scope": [
				"invalid"
			],
			"settings": {
				"background": "#CC565EDF",
				"foreground": "#F8F8F2"
			}
		},
		{
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"background": "#c6797e",
				"foreground": "#F8F8F2"
			}
		},
		{
			"scope": [
				"entity.name.filename"
			],
			"settings": {
				"foreground": "#eabe9a"
			}
		},
		{
			"name": "Markdown Link Underline",
			"scope": [
				"markup.underline",
				"markup.underline.link",
				"markup.underline.link.image"
			],
			"settings": {
				"foreground": "#7ea9a9",
				"fontStyle": "underline"
			}
		},
		{
			"name": "Link text, image alt text",
			"scope": [
				"meta.link.reference.def.restructuredtext",
				"punctuation.definition.directive.restructuredtext",
				"string.other.link.description",
				"string.other.link.title"
			],
			"settings": {
				"foreground": "#6e94b9"
			}
		},
		{
			"name": "Markdown headings",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#ea7599",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Bullets, lists",
			"scope": [
				"beginning.punctuation.definition.list.markdown",
				"beginning.punctuation.definition.quote.markdown",
				"punctuation.definition.link.restructuredtext"
			],
			"settings": {
				"foreground": "#ea7599",
				"fontStyle": "regular"
			}
		},
		{
			"name": "Markdown inline code",
			"scope": [
				"markup.inline.raw",
				"markup.raw.restructuredtext"
			],
			"settings": {
				"foreground": "#97a38f"
			}
		},
		{
			"name": "Markdown bold",
			"scope": "markup.bold",
			"settings": {
				"foreground": "#eabe9a",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown Blockquotes",
			"scope": [
				"entity.name.directive.restructuredtext",
				"markup.quote",
				"markup.italic"
			],
			"settings": {
				"foreground": "#97a38f",
				"fontStyle": "italic"
			}
		},
		{
			"name": "SCSS attibute selector strings",
			"scope": [
					"meta.attribute-selector.scss"
			],
			"settings": {
					"foreground": "#F1FA8C"
			}
		}
	]
}